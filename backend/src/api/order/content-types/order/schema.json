{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item"
    },
    "buyer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Started",
        "Payment Required",
        "In Progress",
        "Finished",
        "Canceled"
      ],
      "default": "Started"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    }
  }
}
