{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::item.item"
    },
    "Buyer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Started",
        "Payment Required",
        "In Progress",
        "Finished",
        "Canceled"
      ]
    },
    "Ordernumber": {
      "type": "uid"
    },
    "Date": {
      "type": "datetime"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    }
  }
}
